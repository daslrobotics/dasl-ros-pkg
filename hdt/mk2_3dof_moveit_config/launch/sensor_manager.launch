<launch>

  <!-- This file makes it easy to include the settings for sensor managers -->  

  <!-- Load the robot specific sensor manager; this sets the moveit_sensor_manager ROS parameter -->
  <arg name="moveit_sensor_manager" default="mk2" />
  <include file="$(find mk2_3dof_moveit_config)/launch/$(arg moveit_sensor_manager)_moveit_sensor_manager.launch" />
  
</launch>
